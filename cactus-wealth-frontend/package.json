{
  "name": "cactus-wealth-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next coverage node_modules/.cache",
    "cleanup": "echo 'Cleanup not needed in Docker'",
    "dev": "next dev -p 3000",
    "dev:clean": "npm run cleanup && next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "prepare": "husky",
    "lint": "next lint",
    "lint:ci": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=verbose --coverage",
    "test:ci:strict": "CI_STRICT_LOGS=1 vitest run --reporter=verbose --coverage",
    "e2e:install": "playwright install --with-deps",
    "e2e": "playwright test",
    "e2e:smoke": "PLAYWRIGHT_JSON_OUTPUT_NAME=results.json PWTEST_SMOKE=1 playwright test --project=chromium-smoke",
    "e2e:full": "PLAYWRIGHT_JSON_OUTPUT_NAME=results.json playwright test --project=chromium-full",
    "e2e:headed": "playwright test --headed --project=chromium --grep @smoke",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "typecheck": "npm run type-check",
    "quality": "npm run format && npm run lint:fix && npm run type-check",
    "lint-staged": "lint-staged",
    "analyze:dead-code": "ts-prune"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "geist": "^1.4.2",
    "lucide-react": "^0.294.0",
    "next": "15.4.1",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.0.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.1.7",
    
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.6",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "ts-prune": {
    "ignore": "playwright\\.config\\.ts|tailwind\\.config\\.ts|postcss\\.config\\.js|next\\.config\\.js|e2e/.*|\\.next/.*|app/(.*/)?(page|layout|template|loading|error|not-found)\\.tsx"
  }
}
